		xmlns:mvc="sap.ui.core.mvc"
		displayBlock="true"
		busy="{appView>/busy}"
		busyIndicatorDelay="{appView>/delay}"
		xmlns="sap.m">
	<SplitApp visible="true"  id="idAppControl"  >
		<masterPages>
			<mvc:XMLView viewName="medicare.view.Master" />
		</masterPages>
		<detailPages>
			<mvc:XMLView viewName="medicare.view.DetailNoObjectsAvailable" />
		</detailPages>
	</SplitApp>
</mvc:View>
